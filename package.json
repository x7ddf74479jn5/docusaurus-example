{
  "name": "docuraurus-portfolio",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint-check": "yarn lint:eslint && yarn check:prettier",
    "lint:eslint": "yarn run -s eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "check:prettier": "yarn run -s prettier --check .",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn check:prettier --write",
    "prepare": "simple-git-hooks || echo 'Can not set git hooks'"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.1",
    "@docusaurus/preset-classic": "2.0.1",
    "@mdx-js/react": "^2.1.2",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.1",
    "@tsconfig/docusaurus": "1.0.6",
    "@types/node": "18.6.4",
    "@typescript-eslint/parser": "5.32.0",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "simple-git-hooks": "2.8.0",
    "textlint": "12.2.1",
    "textlint-rule-preset-ja-technical-writing": "7.0.0",
    "typescript": "4.7.4"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn run -s lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write --loglevel=error",
      "eslint --fix --quiet"
    ],
    "./**/*.{html,css,json}": [
      "prettier --write --loglevel=error"
    ]
  }
}
